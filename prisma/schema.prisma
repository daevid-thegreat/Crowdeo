generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Company {
  id          String   @id @default(uuid())
  name        String
  description String
  passcode    String
  Review      Review[]
}

model Review {
  id        String  @id @default(uuid())
  companyId String
  company   Company @relation(fields: [companyId], references: [id])
  rating    Int
  comment   String
}
