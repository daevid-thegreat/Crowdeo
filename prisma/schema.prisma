generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Company {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  passcode    String
}

model Review {
  id        Int    @id @default(autoincrement())
  companyId Int
  rating    Int
  comment   String
}
